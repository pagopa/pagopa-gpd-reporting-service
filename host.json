{
  "version": "2.0",
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.0.0, 5.0.0)"
  },
  "logging": {
    "fileLoggingMode": "debugOnly",
    "logLevel": {
      "default": "None",
      "Host.Results": "Error",
      "Function.ElaborateOptionsFunction": "Information",
      "Function.RetrieveDetailsFunction": "Information",
      "Function.UpdateOptionFunction": "Information",
      "Host.Aggregator": "Error",
      "Host": "Error"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": false
      }
    }
  }
}
